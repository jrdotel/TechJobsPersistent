<div class="container body-content">

    <style>
        th, td {
            padding: 10px;
        }
    </style>

    <h1>@ViewBag.title</h1>

    <table class="listJobs">
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Employer</th>
            <th>Location</th>
            <th>Skills</th>
        </tr>
        @foreach (var job in ViewBag.jobs)
        {
    <tr>
        <td>@job.JobId</td>
        <td>@job.Name</td>
        <td>@job.EmployerName</td>
        <td>@job.EmployerLocation</td>
        <td>@job.SkillText</td>
    </tr>
        }
    </table>

</div>
